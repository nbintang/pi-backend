generator client {
  provider = "prisma-client-js"
  output   = "../prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Meat {
  id             String   @id @default(cuid())
  name           String
  image          String?
  confidence     Float
  rawProbability Float // <--- tambahkan ini
  category       Category
  createdAt      DateTime @default(now())
}

enum Category {
  FRESH
  SPOILED
}
